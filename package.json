{
    "name": "nodebb-theme-fit",
    "version": "0.0.2",
    "description": "In development - NodeBB theme for FIT.",
    "main": "theme.less",
    "author": {
        "name": "Nazim Ali"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/NazimHAli/nodebb-theme-fit"
    },
    "bugs": {
        "url": "https://github.com/NazimHAli/nodebb-theme-fit/issues"
    },
    "keywords": [
        "nodebb",
        "theme",
        "fit"
    ],
    "nbbpm": {
        "compatibility": "^1.18.0"
    },
    "license": "MIT",
    "husky": {
        "hooks": {
            "pre-commit": "npx lint-staged",
            "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ]
    },
    "dependencies": {
        "bent": "^7.3.12"
    },
    "devDependencies": {
        "@commitlint/cli": "11.0.0",
        "@commitlint/config-angular": "11.0.0",
        "eslint": "8.x",
        "eslint-config-airbnb-base": "14.2.1",
        "eslint-config-nodebb": "0.1.1",
        "eslint-plugin-import": "2.x",
        "husky": "5.0.9",
        "lint-staged": "10.5.4"
    }
}
